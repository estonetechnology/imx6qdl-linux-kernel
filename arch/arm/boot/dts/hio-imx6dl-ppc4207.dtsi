&usdhc1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc1>;
	bus-width = <4>;
	//cd-gpios = <&gpio1 4 0>;
	no-1-8-v;
	keep-power-in-suspend;
	//enable-sdio-wakeup;
	status = "okay";
};

